<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hu.noherczeg.necora.domain.ssa.SSAPaperDepartment" table="tdkdolgozat_tagozat" schema="">
        <id name="id" column="id" access="field"/>
        <property name="name" column="nev" access="field"/>
        <property name="comment" column="megjegyzes" access="field"/>
        <set name="ssaPapers" inverse="true" access="field">
            <key>
                <column name="otdk_tagozat_id"/>
            </key>
            <one-to-many not-found="ignore" class="hu.noherczeg.necora.domain.ssa.SSAPaper"/>
        </set>
    </class>
</hibernate-mapping>