<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hu.noherczeg.necora.domain.field.FieldOfScience" table="tudomanyterulet" schema="">
        <id name="id" column="id" access="field"/>
        <property name="name" column="nev" access="field"/>
        <set name="tenders" inverse="true" table="palyazat_has_tudomanyterulet" access="field">
            <key>
                <column name="tudomanyterulet_id" not-null="true"/>
            </key>
            <many-to-many not-found="ignore" class="hu.noherczeg.necora.domain.tender.Tender"/>
        </set>
        <set name="degreesOfPeople" inverse="true" access="field">
            <key>
                <column name="tudomanyterulet_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="hu.noherczeg.necora.domain.user.PersonsDegree"/>
        </set>
    </class>
</hibernate-mapping>